{% extends 'base.html.twig' %}

{% block title %}Hello CompetencesController!{% endblock %}

{% block body %}


<div class="titre">
    <h1>Mes projets</h1>  

</div> 

<div class="container"> 
    <div class="row">
        {% for projets in projets %} 
        <div class="col-md-4 mt-5 d-flex mt-5"> 
            <div class="card" style="width: 18rem;">
              <img src="{{projets.images}}" class="card-img-top" alt="..."> 
              <div class="card-body">
                <p class="card-text">{{projets.nom}}</p> 
                <p class="card-text">{{projets.lien}}</p> 
              </div>
            </div>
        </div>
        {% endfor %}

        
        

        
    </div>
</div>

{% endblock %}
